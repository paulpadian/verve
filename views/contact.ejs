  <hr \>
  <h3>Parking is located at the red markers:</h3>
  <div id="map"></div>
  <script>
    var map;

    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: {
          lat: 39.767760,
          lng: -104.973817
        },
        zoom: 18
      });
      var marker = new google.maps.Marker({
        position: {
          lat: 39.767397,
          lng: -104.973438
        },
        map: map,
        title: 'Parking'
      });

      var marker = new google.maps.Marker({
        position: {
          lat: 39.767915,
          lng: -104.975291
        },
        map: map,
        title: 'Parking'
      });
    }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= mapkey %>&callback=initMap" async defer></script>

  <hr \>
