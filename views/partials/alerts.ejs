
<% if (alerts.error) { %>
    <% alerts.error.forEach(function(message) { %>
        <div class="container">
            <div class="alert alert-dismissible alert-danger">
            <%= message %>
            </div>
        </div>
    <% }); %>
<% } else if (alerts.success) { %>
    <% alerts.success.forEach(function(message) { %>
        <div class="container">
            <div class="alert alert-dismissible alert-success">
                <%= message %>
            </div>
        </div>
    <% }); %>
<% } %>
